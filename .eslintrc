{
  "root": true,
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": [
      "standard",
      "eslint:recommended",
      "prettier",
      "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}
